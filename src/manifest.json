{
   "background": {
      "service_worker": null
   },
   "action": {
      "default_popup": "popup.html",
      "default_title": "My-Notes"
   },
   "options_page": "options.html",
   "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "96": "icon96.png",
      "128": "icon128.png"
   },
   "description": "My-Notes - Put your story into notes...",
