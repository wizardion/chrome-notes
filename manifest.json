{
  "manifest_version": 2,
  "name": "My-Notes",
  "version": "2.0.0",

  "description": "Work-Note - save your notes where you are.",
  "icons": {
    "128": "/img/Notes_128.png"
  },

  "browser_action": {
    "default_icon": "img/Notes_n2.png",
    "default_title": "My-Notes",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js",
      "js/migration.js"
    ],
    "persistent": true
  },
  "key": "",
  "permissions": [ 
    "tabs", "http://*/*", "https://*/*", "storage"
  ],
  "options_page": "options.html",
  "update_url": "http://clients2.google.com/service/update2/crx"
}